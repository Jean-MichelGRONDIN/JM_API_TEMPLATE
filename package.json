        "@types/express": "^4.17.12",
        "@types/jest": "^26.0.23",
        "@types/mocha": "^8.2.2",
        "@types/node": "^15.12.2",
        "@types/pg": "^8.6.0",
        "@types/uuid": "^8.3.1",
        "@typescript-eslint/eslint-plugin": "^4.26.1",
        "@typescript-eslint/parser": "^4.26.1",
        "chai": "^4.3.4",
        "chai-http": "^4.3.0",
        "concurrently": "^6.2.0",
        "eslint": "^7.28.0",
        "mocha": "^9.0.0",
        "nodemon": "^2.0.7",
        "nyc": "^15.1.0",
        "prettier": "^2.3.2",
        "ts-node": "^10.0.0",
